{
	"name": "astro-integration-kit",
	"version": "0.19.0",
	"description": "A package that contains utilities to help you build Astro integrations.",
	"type": "module",
	"sideEffects": false,
	"exports": {
		".": {
			"types": "./dist/core/index.d.ts",
			"default": "./dist/core/index.js"
		},
		"./plugins": {
			"types": "./dist/plugins/index.d.ts",
			"default": "./dist/plugins/index.js"
		},
		"./dev": {
			"types": "./dist/dev/index.d.ts",
			"default": "./dist/dev/index.js"
		}
	},
	"files": [
		"dist",
		"types"
	],
	"scripts": {
		"dev": "tsup --watch",
		"build": "tsup",
		"test": "vitest run",
		"prepublishOnly": "pnpm build"
	},
	"keywords": [
		"withastro",
		"astro",
		"utils",
		"utilities"
	],
	"author": "florian-lefebvre",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/florian-lefebvre/astro-integration-kit.git"
	},
	"bugs": "https://github.com/florian-lefebvre/astro-integration-kit/issues",
	"homepage": "https://github.com/florian-lefebvre/astro-integration-kit#readme",
	"license": "MIT",
	"peerDependencies": {
		"astro": "^4.14.0 || ^5.0.0"
	},
	"devDependencies": {
		"@types/node": "^20.12.12",
		"tsup": "^8.0.2",
		"typescript": "^5.4.5",
		"vite": "^5.2.11",
		"vitest": "^1.6.0"
	},
	"dependencies": {
		"pathe": "^1.1.2"
	}
}
