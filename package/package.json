{
	"name": "astro-integration-kit",
	"version": "0.18.0",
	"description": "A package that contains utilities to help you build Astro integrations.",
	"author": {
		"email": "contact@florian-lefebvre.dev",
		"name": "Florian Lefebvre",
		"url": "https://florian-lefebvre.dev"
	},
	"license": "MIT",
	"keywords": ["withastro", "astro", "utils", "utilities"],
	"homepage": "https://github.com/florian-lefebvre/astro-integration-kit",
	"publishConfig": {
		"access": "public"
	},
	"sideEffects": false,
	"exports": {
		".": {
			"types": "./dist/core/index.d.ts",
			"default": "./dist/core/index.js"
		},
		"./plugins": {
			"types": "./dist/plugins/index.d.ts",
			"default": "./dist/plugins/index.js"
		},
		"./dev": {
			"types": "./dist/dev/index.d.ts",
			"default": "./dist/dev/index.js"
		}
	},
	"files": ["dist", "types"],
	"scripts": {
		"dev": "tsup --watch",
		"build": "tsup",
		"test": "vitest run"
	},
	"type": "module",
	"peerDependencies": {
		"astro": "^4.14.0 || ^5.0.0"
	},
	"devDependencies": {
		"@types/node": "^20.12.12",
		"tsup": "^8.0.2",
		"typescript": "^5.4.5",
		"vite": "^5.2.11",
		"vitest": "^1.6.0"
	},
	"dependencies": {
		"pathe": "^1.1.2"
	}
}
